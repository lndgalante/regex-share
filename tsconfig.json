{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib": ["es6"],
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strictNullChecks": false
  },
  "exclude": ["node_modules", ".vscode-test"],
  "types": ["mocha"]
}
